{
    "name": "temporal-hello-world",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "tsc --build",
        "build.watch": "tsc --build --watch",
        "lint": "eslint .",
        "start": "ts-node src/workers/worker.ts",
        "start.watch": "nodemon src/workers/worker.ts",
        "workflow": "ts-node src/client.ts",
        "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "test": "mocha --exit --require ts-node/register --require source-map-support/register src/mocha/*.test.ts",
        "client": "ts-node src/client.ts"
    },
    "nodemonConfig": {
        "execMap": {
            "ts": "ts-node"
        },
        "ext": "ts",
        "watch": [
            "src"
        ]
    },
    "dependencies": {
        "@temporalio/activity": "^1.9.0",
        "@temporalio/client": "^1.11.6",
        "@temporalio/common": "^1.11.6",
        "@temporalio/worker": "^1.11.6",
        "@temporalio/workflow": "^1.11.5",
        "axios": "^1.7.9",
        "child_process": "^1.0.2",
        "express": "^4.21.1",
        "fs": "^0.0.1-security",
        "lodash": "^4.17.21",
        "nanoid": "^3.3.8",
        "node": "^23.5.0",
        "temporal-hello-world": "file:",
        "temporalio": "^1.9.3",
        "xml-js": "^1.6.11"
    },
    "devDependencies": {
        "@temporalio/testing": "^1.9.0",
        "@tsconfig/node16": "^1.0.0",
        "@types/express": "^5.0.0",
        "@types/lodash": "^4.17.13",
        "@types/mocha": "8.x",
        "@types/node": "^16.11.43",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-deprecation": "^1.2.1",
        "mocha": "8.x",
        "nodemon": "^2.0.12",
        "prettier": "^2.8.8",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.5"
    }
}
